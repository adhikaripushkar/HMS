@model HospitalManagementSystem.Models.IpdMrMainTestModel





@for (int i = 0; i < Model.SectionTestCheckBoxList.Count; i++)
{
    @Html.CheckBoxFor(model => model.SectionTestCheckBoxList[i].isSelected)
    @Html.DisplayFor(model => model.SectionTestCheckBoxList[i].TestName, new { @class = "TestID" })

    
    @Html.HiddenFor(model => model.SectionTestCheckBoxList[i].TestID, new { @class = "TestID" })
    @Html.HiddenFor(model => model.SectionTestCheckBoxList[i].TestName, new { @class = "TestID" })   
    
}
