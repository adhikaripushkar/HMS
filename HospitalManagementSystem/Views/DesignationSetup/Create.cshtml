@model HospitalManagementSystem.Models.DesignationSetupModel

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_PayRollLayout.cshtml";
}



<h2 style="text-align:center;">Designation Setup</h2>
<div class="contentWrapper">

@using (Html.BeginForm("Create", "DesignationSetup", FormMethod.Post, new { @class = "bs-docs-example form-inline" }))
{
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>Create</legend>
         <div class="row-fluid singleline">

       
      

             <label>
                 <span class="size">    @Html.ValidationMessageFor(model => model.LevelID)</span>
                 <div class="editor-label">
                     @Html.LabelFor(model => model.LevelID)
                 </div>
                 <div class="editor-field">
                     @Html.EditorFor(model => model.LevelID)
        
                 </div>
             </label>
                <label>
     <span class="size">    @Html.ValidationMessageFor(model => model.Designation)</span>
        <div class="editor-label">
            @Html.LabelFor(model => model.Designation)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Designation)
        
        </div>
    </label>
       

        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index", null, new { @id="linkNewBack"})
</div>
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
