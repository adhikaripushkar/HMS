@model HospitalManagementSystem.Models.OpdMedicalDetailModel

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_OpdLayOut.cshtml";
}

<div class="contentWrapper">
    <div class="boxHeadingPanel"><h1>Agent Details</h1></div>
<table class="table table-hover tablesorter">
    <thead>
    <tr>
        <th>
          Patient Name
        </th>
       
        <th>
            @Html.DisplayNameFor(model => model.ManPowerId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PreHolo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Commission)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreatedDate)
        </th>
      
     
    </tr>
        </thead>
    <tbody>
@foreach (var item in Model.OpdMedicalDetailList) {
    <tr>
        <td>
            @HospitalManagementSystem.Utility.OpdPatientName(item.OpdMasterId)
        </td>
        <td>
            @HospitalManagementSystem.Utility.GetManpowerName(item.ManPowerId)
        </td>
       
        <td>
          @Html.DisplayFor(modelItem => item.PreHolo)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.Commission)
        </td>
       <td>
               @Html.DisplayFor(modelItem => item.CreatedDate)
        </td>
        
       @* <td>
            @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
            @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
            @Html.ActionLink("Delete", "DeleteDetail", new { id=item.StockItemPurchaseDetailId })
        </td>*@
    </tr>
}
</tbody>
</table>
    <div class="boxFooter">
                   
                   <div id="pager" style="">
            <form>
            <img src="@Url.Content("~/Content/images/first.png")" class="first" />
            <img src="@Url.Content("~/Content/images/prev.png")" class="prev" />
            <input class="pagedisplay" type="text" style="width:60px;height:24px;">
            <img src="@Url.Content("~/Content/images/next.png")" class="next" />
            <img src="@Url.Content("~/Content/images/last.png")" class="last" />
            <select class="pagesize valid" style="width:70px;height:30px;"">
                <option selected="selected" value="10">10</option>
                <option value="5">5</option>
                <option value="20">20</option>
                <option value="30">30</option>
                <option value="40">40</option>
            </select>
            </form>
            </div>

               </div>
    <br />
    <br />
    <p>
    @Html.ActionLink("Back to List", "calulateagenttotal")
</p>
    </div>
