@model Hospital.Models.VDCMUNModel
@{
    ViewBag.Title = "Create";
}

<div class="block-header">
                <h4 class="block-title">Create</h4>
              </div>
 <div class="well" style="border-radius:0;">
              <div class="side-by-side clearfix">
           

@using (Html.BeginForm(null, null, FormMethod.Post, new { @class="form-horizontal"}))
{
     @Html.ValidationSummary(true)
    //
    <div class="clearfix">
                  <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12">
                      <div class="contGroup">
                         @Html.LabelFor(model => model.DistrictID)
                          @Html.DropDownListFor(x =>x.DistrictID,Hospital.Utility.GetDistrictList(),new { @class="form-control chosen-select"})
                            @Html.ValidationMessageFor(model => model.DistrictID)

                      </div>
                    </div>
                    
          <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12">
                      <div class="contGroup">
                         @Html.LabelFor(model => model.VdcMunNameEng)
                          @Html.TextBoxFor(x =>x.VdcMunNameEng,new { @class="form-control"})
                            @Html.ValidationMessageFor(model => model.VdcMunNameEng)

                      </div>
                    </div>
            </div>
    //@Html.Partial("VUC_Add");   
     <div class="bottom_btn">
                   
        <a href="@Url.Action("Index","VDCMUN")" class="btn btn-sm btn-warning"><i class="fa fa-undo"></i> Go Back</a>

      
                    <button type="submit" id="" name="" class="btn btn-success"><i class="fa fa-save"></i> Save</button>
                    
                  </div>
    
  
}
    
         </div>
     </div>



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}

