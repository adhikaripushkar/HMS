<script>
    $(".linkAddress").click(function(e)
    {
        e.preventDefault();
        var data = $('#EmergencyMasterId').val();
        var url = this.href;
        $.get(url, { id: data }, function (res) {
            $("#load").html(res);
        });
    })
    </script>
<script>
    $("#sumtid").click(function (e) {
        e.preventDefault();
        var form = $('#EditForComplainEntryId');
        var url = form.attr("action");
        var formData = form.serialize();
        $.post(url, formData, function (data) {
            $('#EmergencyComplainEntryId').empty();
            $('#EmergencyComplainEntryId').html(data);
            $('#EditForComplainEntryId').remove();
        });
    });
    </script>

     

    





